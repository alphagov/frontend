{{#show_result?}}
<li>
  <h3><a href="{{link}}" {{#external}}rel="external"{{/external}}
       data-ecommerce-row
       data-ecommerce-path="{{link}}"
       data-ecommerce-content-id="{{content_id}}">{{{title_with_highlighting}}}</a></h3>

  {{#debug_score}}
    <p class="debug-link">
      {{link}}
    </p>
    <p class="debug-info">
      <span>Score: {{es_score}}</span>
      <span>Format: {{#government}}government{{/government}} {{format}}</span>
    </p>
  {{/debug_score}}

  {{#external}}
    <p class="meta">
      <span class="visuallyhidden">Part of </span>
      <span class="url">{{ display_link }}</span>
    </p>
  {{/external}}

  {{#metadata_any?}}
    <ul class="attributes">
      {{#metadata}}
        <li> {{{.}}} </li>
      {{/metadata}}
    </ul>
  {{/metadata_any?}}

  {{#historic?}}
    {{#government_name}}
    <p class="historic">
      First published during the {{government_name}}
    </p>
    {{/government_name}}
  {{/historic?}}

  <p>{{{description_with_highlighting}}}</p>

  {{#sections_present?}}
    <ul class="sections">
      {{#sections}}
        <li><a href="{{link}}#{{hash}}">{{title}}</a></li>
      {{/sections}}
    </ul>
  {{/sections_present?}}

  {{#examples_present?}}
    <ul class="examples">
      {{#examples}}
        <li>
          <h4><a href="{{link}}">{{title}}</a></h4>
          <p>{{description}}</p>
        </li>
      {{/examples}}

      {{#suggested_filter_present?}}
        <li>
          <h4 class="see-all"><a href="{{suggested_filter_link}}">{{suggested_filter_title}}</a><h4>
        </li>
      {{/suggested_filter_present?}}
    </ul>
  {{/examples_present?}}

  {{^examples_present?}}
    {{#suggested_filter_present?}}
      <h4 class="see-all"><a href="{{suggested_filter_link}}">{{suggested_filter_title}}</a><h4>
    {{/suggested_filter_present?}}
  {{/examples_present?}}

  {{#carers_allowance?}}
    <div class="sitelinks">
      <div class="sitelink">
        <a href="/carers-allowance">How it works</a>
        <p>You could get £62.70 a week if you care for someone at least...</p>
      </div>

      <div class="sitelink">
        <a href="/carers-allowance/how-to-claim">Make a claim</a>
        <p>Before you apply make sure you have your: National Insurance...</p>
      </div>

      <div class="sitelink">
        <a href="/carers-allowance/eligibility">Eligibility</a>
        <p>The person you care for must already get one of these...</p>
      </div>

      <div class="sitelink">
        <a href="/carers-allowance/further-information">Change in circumstances</a>
        <p>You must report any change in your circumstances if you’re...</p>
      </div>

      <div class="sitelink">
        <a href="/carers-allowance/effect-on-other-benefits">Effect on other benefits</a>
        <p>Carer’s Allowance can affect the other benefits that both you...</p>
      </div>

      <div class="clearfix"></div>
      <a class="more-results" href="/carers-allowance">More results from GOV.UK »</a>
    </div>
  {{/carers_allowance?}}

</li>
{{/show_result?}}
